{
  "name": "pixi-holga",
  "version": "0.5.0",
  "description": "Pixi powered camera",
  "main": "lib/pixi-holga.js",
  "module": "lib/pixi-holga.mjs",
  "jsnext:main": "lib/pixi-holga.mjs",
  "unpkg": "dist/pixi-holga.min.js",
  "jsdelivr": "dist/pixi-holga.min.js",
  "scripts": {
    "clean": "rimraf dist lib",
    "test": "tape -r buba/register src/**/*.spec.js",
    "prepublish": "npm run build",
    "prepack": "npm run build",
    "watch": "rollup -c -w",
    "prebuild": "npm run clean",
    "build": "rollup -c"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "files": [
    "dist",
    "lib"
  ],
  "author": "Matt Styles",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mattstyles/pixi-holga.git"
  },
  "keywords": [
    "pixi",
    "pixi.js",
    "camera",
    "viewport",
    "view"
  ],
  "bugs": {
    "url": "https://github.com/mattstyles/pixi-holga/issues"
  },
  "homepage": "https://github.com/mattstyles/pixi-holga#readme",
  "lint-staged": {
    "*.js": [
      "standard"
    ],
    "*.jsx": [
      "standard"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/lib/"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "buba": "^4.0.2",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.23.1",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^6.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "standard": "^14.3.1",
    "tape": "^4.11.0"
  },
  "dependencies": {
    "eventemitter3": "^4.0.0",
    "mathutil": "^2.3.0"
  }
}
